# Config file for the @EXPORT_TYPE@ cryptominisat Package
# It defines the following variables
#  MIS_INCLUDE_DIRS - include directories for cryptominisat5
#  MIS_LIBRARIES    - libraries to link against
#  MIS_EXECUTABLE   - the cryptominisat executable

# Compute paths
get_filename_component(MIS_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(MIS_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
include("${MIS_CMAKE_DIR}/@MIS_TARGETS_FILENAME@")

# These are IMPORTED targets created by @MIS_TARGETS_FILENAME@
set(MIS_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
